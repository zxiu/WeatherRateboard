<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>
			org.springframework.web.servlet.DispatcherServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>weatherforecast10day</servlet-name>
		<servlet-class>
			org.springframework.web.servlet.DispatcherServlet
		</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
		<servlet>
		<servlet-name>initiatecities</servlet-name>
		<servlet-class>
			org.springframework.web.servlet.DispatcherServlet
		</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/welcome.jsp</url-pattern>
		<url-pattern>/welcome.html</url-pattern>
		<url-pattern>*.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>weatherforecast10day</servlet-name>
		<url-pattern>/weatherforecast10day.jsp</url-pattern>
		<url-pattern>/weatherforecast10day.html</url-pattern>
	</servlet-mapping>
	
		<servlet-mapping>
		<servlet-name>initiatecities</servlet-name>
		<url-pattern>/initiatecities</url-pattern>
	</servlet-mapping>
	

</web-app>
